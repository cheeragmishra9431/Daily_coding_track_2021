    #include<bits/stdc++.h>
    using namespace std;
    
    int getsum(vector<vector<int> >& matrix, int i, int j ,vector<vector<int> >& matrix1){
        int sum=0;
        if(i==0){
            if(j==0){
                sum=matrix[i+1][j]+matrix[i][j+1];
                matrix1[i+1][j]=0;
                matrix1[i][j+1]=0;
            }
            if(j==matrix.size()-1){
                sum=matrix[i+1][j]+ matrix[i][j-1];
                matrix1[i+1][j]=0;
                matrix1[i][j-1]=0;
                
            }
            else{
                sum=matrix[i+1][j]+matrix[i][j+1]+ matrix[i][j-1];
                matrix1[i+1][j]=0;
                matrix1[i][j+1]=0;
                matrix1[i][j-1]=0;
            }
        }
        if(j==0){
            if(i==matrix.size()-1){
                sum=matrix[i][j+1]+matrix[i-1][j];
                matrix1[i][j+1]=0;
                matrix1[i-1][j]=0;
            }
            if(j==matrix[i].size()-1){
                sum=matrix[i+1][j]+ matrix[i][j-1];
                matrix1[i+1][j]=0;
                matrix1[i][j-1]=0;
            }
            else{
                sum=matrix[i+1][j]+matrix[i][j+1]+ matrix[i-1][j];
                matrix1[i+1][j]=0;
                matrix1[i][j+1]=0; 
                matrix1[i-1][j]=0;
            }
        }
        if(j==matrix[i].size()-1){
            if(i==matrix.size()-1){
                sum=matrix[i][j-1]+matrix[i-1][j];
                matrix1[i][j-1]=0;
                matrix1[i-1][j]=0;
            }
            
            else{
                sum=matrix[i+1][j]+matrix[i][j-1]+ matrix[i-1][j];
                matrix1[i+1][j]=0;
                matrix1[i][j-1]=0; 
                matrix1[i-1][j]=0;
            }
        }
        if(i==matrix.size()-1){
            sum=matrix[i][j-1]+matrix[i][j+1]+matrix[i-1][j];
            matrix1[i][j-1]=0;
            matrix1[i][j+1]=0;
            matrix1[i-1][j]=0;
        }
        else{
             sum=matrix[i+1][j]+matrix[i][j-1]+ matrix[i-1][j]+matrix[i][j+1];
                matrix1[i+1][j]=0;
                matrix1[i][j-1]=0; 
                matrix1[i-1][j]=0;
                matrix1[i][j+1]=0;
        }
        return sum;
        
    }
    void MakeZeros(vector<vector<int> >& matrix) {
        if(matrix.size()==0){
            return;
        }
        if(matrix.size()==1){
           if(matrix[0].size()==1){
               return ;
           }
        }
        
        // Code here
        vector<vector<int> > matrix1;
        matrix1=matrix;
        if(matrix.size()==1){
            for (auto i = 0; i != matrix[0].size(); ++i){
                if(matrix[0][i]==0){
                     if(i==0){
                         matrix1[0][i]=matrix[0][i+1];
                         matrix1[0][i+1]=0;
                     }
                     if(i==matrix.size()-1){
                         matrix1[0][i]=matrix[0][i-1];
                         matrix1[0][i-1]=0;
                     }
                     else{
                         matrix1[0][i]=matrix[0][i+1]+matrix[0][i-1];
                         matrix1[0][i+1]=0;
                         matrix1[0][i-1]=0;
                     }
                    
                }
                
            }
            matrix=matrix1;
            return;
        }    
        if(matrix[0].size()==1){
            for (auto i = 0; i != matrix.size(); ++i){
                if(matrix[i][0]==0){
                     if(i==0){
                         matrix1[i][0]=matrix[i+1][0];
                         matrix1[i+1][0]=0;
                     }
                     if(i==matrix.size()-1){
                         matrix1[i][0]=matrix[i-1][0];
                         matrix1[i-1][0]=0;
                     }
                     else{
                         matrix1[i][0]=matrix[i+1][0]+matrix[i-1][0];
                         matrix1[i+1][0]=0;
                         matrix1[i-1][0]=0;
                     }
                    
                }
                
            }
            matrix=matrix1;
            return;
        }    
        for (auto i = 0; i != matrix.size(); ++i)
        {
            
            for (auto j = 0; j != matrix[i].size(); ++j)
                if(matrix[i][j]==0){
                    
                
                    int sum=getsum(matrix,i,j,matrix1);
                    matrix1[i][j]=sum;
                }
    
            
        }
        matrix=matrix1;
    }
};